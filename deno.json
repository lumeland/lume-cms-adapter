{
  "lock": false,
  "imports": {
    "lume/": "https://deno.land/x/lume@v3.0.9/",
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.13.0-beta.1/"
  },
  "tasks": {
    "update-deps": {
      "description": "Update dependencies",
      "command": "deno run -A --quiet 'https://deno.land/x/nudd@v0.2.9/cli.ts' update *.ts deno.json"
    }
  },
  "exclude": [
    "CHANGELOG.md"
  ],
  "lint": {
    "rules": {
      "exclude": [
        "no-import-prefix"
      ]
    }
  }
}
